  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/stillTest.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch SunCalc (TS)",
      "program": "${workspaceFolder}/src/lib/suncalc/index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "runtimeArgs": ["--env-file=.env"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
