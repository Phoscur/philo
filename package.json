{
  "name": "philo-philm",
  "version": "1.2.3",
  "description": "Photo & Timelapse Bot",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run run-built",
    "run-built": "node --env-file=.env dist/main.js",
    "build": "tsc -b --clean",
    "watch": "tsx watch --env-file=.env src/index.ts",
    "still-test": "tsx --env-file=.env src/stillTest.ts",
    "upload-test": "tsx --env-file=.env src/uploadTest.ts",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phoscur/philo.git"
  },
  "keywords": [
    "photo",
    "raspberry-pi",
    "telegram",
    "telegraf",
    "bot",
    "timelapse"
  ],
  "author": "Phoscur",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Phoscur/philo/issues"
  },
  "homepage": "https://github.com/Phoscur/philo#readme",
  "dependencies": {
    "@aws-sdk/client-glacier": "^3.312.0",
    "@joist/di": "^3.2.1",
    "@octokit/rest": "^20.1.1",
    "axios": "^1.3.5",
    "dayjs": "^1.11.7",
    "discord.js": "^14.9.0",
    "isomorphic-git": "^1.23.0",
    "libcamera": "^1.0.7",
    "libsodium-wrappers": "^0.7.13",
    "suncalc": "^1.9.0",
    "telegraf": "^4.12.2"
  },
  "optionalDependencies": {
    "node-dht-sensor": "^0.4.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^20.14.2",
    "@types/suncalc": "^1.9.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5"
  }
}
